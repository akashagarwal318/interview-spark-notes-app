{
  "name": "interview-assistant-backend",
  "version": "1.0.0",
  "description": "Backend API for Interview Assistant Application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "reset-admin": "node scripts/resetAdmin.mjs",
    "seed": "node scripts/seedData.js --drop",
    "seed:preview": "node scripts/seedData.js --preview --limit=5",
    "seed:limit": "node scripts/seedData.js --drop --limit=10",
    "seed:clean": "node scripts/seedData.js --drop --skip-tags"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "interview",
    "questions",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "Interview Assistant Team",
  "license": "MIT"
}
